.title{
  font-size: 25px;
  text-align: center;
  margin-top: 30px;
}
.date{
  margin-top: 20px;
  position: relative;
  left: 300px;
  font-size: 15px
}
.article{
  margin-top: 20px;
  margin-left: 10px;
  margin-right: 10px;
}
.img{
  margin-top: 20px;
  display:flex;                 
  justify-content: center;   
}
.location{
  font-size: 15px;
  display: flex;
  justify-content: center;
}
.round-click{
    height: 120rpx;
    width: 120rpx;
    background-color: #73cbee;
    border-radius: 100%;
    position: fixed;
    bottom: 50rpx;
    right: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9;
}
.round-click navigator{
    font-size: 40rpx;
    max-width: 80rpx;
    color: #fff;
    text-align: center;
}
button[type="default"] {
  border-radius: 0px;
  background-color: #fff;
  color: #1296db;
}

button[type="default"]::after {
  border: none;
}
